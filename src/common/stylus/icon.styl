@font-face
  font-family 'damu'
  src  url('../fonts/damu.eot?lbmi6k');
  src  url('../fonts/damu.eot?lbmi6k#iefix') format('embedded-opentype'),
    url('../fonts/damu.ttf?lbmi6k') format('truetype'),
    url('../fonts/damu.woff?lbmi6k') format('woff'),
    url('../fonts/damu.svg?lbmi6k#damu') format('svg')
  font-weight normal
  font-style normal


[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'damu' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

